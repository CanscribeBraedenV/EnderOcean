[application]

config/name="ENDER OCEAN"
config/description="Your mission : \"Clean the Ocean!\""
run/main_scene="res://scenes/ui/menu.tscn"
boot_splash/image="res://assets/logo_ender.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.129412, 0.121569, 0.164706, 1 )
config/icon="res://assets/logo_ender.png"
config/windows_native_icon="res://assets/logo_ender.ico"

[autoload]

Globals="*res://scripts/globals.gd"
SceneLoader="*res://scripts/scene_loader.gd"
LevelLoader="*res://scripts/level/level_loader.gd"
Leaderboard="*res://scripts/networking/leaderboard.gd"
SaveManager="*res://scripts/io/save_manager.gd"
RadioSounds="*res://scripts/sounds/radio_sounds.gd"
Localization="*res://scripts/localization/localization.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/width=1920
window/size/height=1080
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/zylann.hterrain/plugin.cfg", "res://addons/zylann.scatter/plugin.cfg" )

[gui]

theme/custom_font="res://assets/fonts/Roboto/Roboto-Regular.tres"

